<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
   
  <display-name>Archetype Created Web Application</display-name>
 <servlet>
   <servlet-name>Login</servlet-name>
   <servlet-class>com.csvmodifer.Login</servlet-class>
</servlet>
<servlet>
   <servlet-name>Logout</servlet-name>
   <servlet-class>com.csvmodifer.Logout</servlet-class>
</servlet>
<servlet>
   <servlet-name>Home</servlet-name>
   <servlet-class>com.csvmodifer.Start</servlet-class>
</servlet>
<servlet>
   <servlet-name>SetToken</servlet-name>
   <servlet-class>com.csvmodifer.SetToken</servlet-class>
</servlet>
<servlet>
   <servlet-name>VerifyToken</servlet-name>
   <servlet-class>com.csvmodifer.VerifyToken</servlet-class>
</servlet>
 <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
   <servlet-name>Logout</servlet-name>
   <url-pattern>/logout</url-pattern>
</servlet-mapping>
<servlet-mapping>
   <servlet-name>Home</servlet-name>
   <url-pattern>/</url-pattern>
</servlet-mapping>
<servlet-mapping>
   <servlet-name>SetToken</servlet-name>
   <url-pattern>/settoken</url-pattern>
</servlet-mapping>
<servlet-mapping>
   <servlet-name>VerifyToken</servlet-name>
   <url-pattern>/verifytoken</url-pattern>
</servlet-mapping>
<welcome-file-list>
   <welcome-file>/</welcome-file>
</welcome-file-list>
</web-app>
